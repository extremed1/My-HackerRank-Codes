i = 4
d = 4.0
s = 'HackerRank '
# Declare second integer, double, and String variables.
a = 0
b = 0.0
c = " "
# Read and save an integer, double, and String to your variables.
a = int(input())
b = float(input())
c = str(input())
# Print the sum on a new line.
print(i + a)
print(d + b)
print(s + c)


